<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="qanda">
  	<select id="findAll" resultType="qanda">
  		select * from qanda
  	</select>
  	<select id="findByQno" resultType="qanda">
  		select * from qanda where qno= #{qno}
  	</select>
  </mapper>