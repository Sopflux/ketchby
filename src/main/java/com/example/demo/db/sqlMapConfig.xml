<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <properties resource="com/example/demo/db/db.properties"/>
  <typeAliases>
  	<typeAlias type="com.example.demo.entity.Account" alias="account"/>
  	<typeAlias type="com.example.demo.entity.Image" alias="image"/>
  	<typeAlias type="com.example.demo.entity.Review" alias="review"/>
  	<typeAlias type="com.example.demo.entity.Accclub" alias="accclub"/>
  	<typeAlias type="com.example.demo.entity.Acclike" alias="acclike"/>
  	<typeAlias type="com.example.demo.entity.Reclass" alias="reclass"/>
  	<typeAlias type="com.example.demo.entity.Confirm" alias="confirm"/>
  	<typeAlias type="com.example.demo.entity.Reservation" alias="reservation"/>
  	<typeAlias type="com.example.demo.entity.Time" alias="time"/>
  	<typeAlias type="com.example.demo.entity.ClDate" alias="cldate"/>
  	<typeAlias type="com.example.demo.entity.Class" alias="class"/>
  	<typeAlias type="com.example.demo.entity.PaymentInfo" alias="payment"/>
  	<typeAlias type="com.example.demo.entity.Follow" alias="follow"/>
  </typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/example/demo/db/accountMapper.xml"/>
    <mapper resource="com/example/demo/db/reservationMapper.xml"/>
  </mappers>
</configuration>
