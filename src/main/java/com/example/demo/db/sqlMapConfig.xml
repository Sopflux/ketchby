<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <properties resource="com/example/demo/db/db.properties"/>
  <typeAliases>
  	<typeAlias type="com.example.demo.entity.Account" alias="account"/>
  	<typeAlias type="com.example.demo.entity.QandA" alias="qanda"/>
  	<typeAlias type="com.example.demo.entity.BoardHot" alias="boardhot"/>
  	<typeAlias type="com.example.demo.entity.Board" alias="board"/>
  	<typeAlias type="com.example.demo.entity.Comments" alias="comments"/>
  	<typeAlias type="com.example.demo.entity.ClubMain" alias="clubmain"/>
  	<typeAlias type="com.example.demo.entity.Club" alias="club"/>
  	<typeAlias type="com.example.demo.entity.Scategory" alias="scategory"/>
  	<typeAlias type="com.example.demo.entity.Slocation" alias="slocation"/>
  	<typeAlias type="com.example.demo.entity.ClubDetail" alias="clubdetail"/>
  	<typeAlias type="com.example.demo.entity.Clubapplication" alias="clubapplication"/>
  	<typeAlias type="com.example.demo.entity.Notice" alias="notice"/>
  	<typeAlias type="com.example.demo.entity.QnainsertVO" alias="qnainsert"/>
  	<typeAlias type="com.example.demo.entity.AdminQnainsertVO" alias="adminqnainsert"/>
  	<typeAlias type="com.example.demo.entity.ClassDetailVO" alias="classDetail"/>
  	<typeAlias type="com.example.demo.entity.Class" alias="class"/>
  	<typeAlias type="com.example.demo.entity.Time" alias="time"/>
  	<typeAlias alias="review" type="com.example.demo.entity.Review"/>
  	<typeAlias type="com.example.demo.entity.ReviewInfoVO" alias="reviewinfo"/> 	
  	<typeAlias alias="image" type="com.example.demo.entity.Image"/>
   <typeAlias alias="accclub" type="com.example.demo.entity.Accclub"/>
   <typeAlias alias="acclike" type="com.example.demo.entity.Acclike"/>
   <typeAlias alias="reclass" type="com.example.demo.entity.Reclass"/>
   <typeAlias alias="reservation" type="com.example.demo.entity.Reservation"/>
     <typeAlias type="com.example.demo.entity.Feed" alias="feed"/>
     <typeAlias type="com.example.demo.entity.Likes" alias="likes"/>
     <typeAlias type="com.example.demo.entity.Feedcommentary" alias="feedcommentary"/>
     <typeAlias type="com.example.demo.entity.Bcategory" alias="bcategory"/>
     <typeAlias type="com.example.demo.entity.Blocation" alias="blocation"/>
     <typeAlias type="com.example.demo.entity.Place" alias="place"/>
     <typeAlias type="com.example.demo.entity.Favorite" alias="favorite"/>
     <typeAlias type="com.example.demo.entity.Quit" alias="quit"/>
     <typeAlias type="com.example.demo.entity.Reason" alias="reason"/>
  	
  	
  </typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>
      </dataSource>
    </environment>
    
  </environments>
  <mappers>
    <mapper resource="com/example/demo/db/accountMapper.xml"/>
    <mapper resource="com/example/demo/db/qandaMapper.xml"/>
    <mapper resource="com/example/demo/db/boardhotMapper.xml"/>
    <mapper resource="com/example/demo/db/boardMapper.xml"/>
    <mapper resource="com/example/demo/db/commentsMapper.xml"/>
    <mapper resource="com/example/demo/db/clubmainMapper.xml"/>
    <mapper resource="com/example/demo/db/clubMapper.xml"/>
    <mapper resource="com/example/demo/db/scategoryMapper.xml"/>
    <mapper resource="com/example/demo/db/slocationMapper.xml"/>
    <mapper resource="com/example/demo/db/clubdetailMapper.xml"/>
    <mapper resource="com/example/demo/db/clubapplicationMapper.xml"/>
    <mapper resource="com/example/demo/db/noticeMapper.xml"/>
    <mapper resource="com/example/demo/db/qnainsertMapper.xml"/>
    <mapper resource="com/example/demo/db/AdminAccountMapper.xml"/>
    <mapper resource="com/example/demo/db/AdminNoticeMapper.xml"/>
    <mapper resource="com/example/demo/db/AdminQnaMapper.xml"/>
    <mapper resource="com/example/demo/db/ClassDetailMapper.xml"/>
    <mapper resource="com/example/demo/db/ClassOpenMapper.xml"/>
    <mapper resource="com/example/demo/db/feedMapper.xml"/>
    <mapper resource="com/example/demo/db/favorMapper.xml"/>
  </mappers>
</configuration>
